<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
        Theme overlay styles for specific widgets.
        Preserve alphabetical order.
    -->

    <style name="MaterialThemeOverlay.Button.Outlined" parent="ThemeOverlay.Material3.Button.TextButton">
        <!-- nothing special here -->
    </style>

    <style name="MaterialThemeOverlay.Button.Outlined.OnBackground">
        <item name="colorOnContainer">?attr/colorOnBackground</item>
    </style>

    <style name="MaterialThemeOverlay.Button.Tonal" parent="">
        <item name="colorOnContainer">?attr/colorOnPrimaryContainer</item>
        <item name="colorContainer">?attr/colorPrimaryContainer</item>
    </style>

    <style name="MaterialThemeOverlay.Button.Tonal.SurfaceVariant" parent="">
        <item name="colorOnContainer">?attr/colorOnSurfaceVariant</item>
        <item name="colorContainer">?attr/colorSurfaceVariant</item>
    </style>

    <style name="MaterialThemeOverlay.Chip.Outlined" parent="">
        <item name="colorSurface">@color/transparent</item>
        <item name="colorSecondaryContainer">@color/csl_color_on_background_disabled_background</item>
        <item name="colorOnSurfaceVariant">?attr/colorOnBackground</item>
        <item name="colorOnSecondaryContainer">?attr/colorOnBackground</item>
        <item name="colorOutline">@color/material_on_background_disabled</item>
    </style>

</resources>